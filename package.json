{
  "name": "journeys",
  "version": "1.0.0",
  "description": "Cycle journey data recorder",
  "main": "index.js",
  "scripts": {
    "build:server": "BABEL_ENV=node babel src -d dist",
    "build:client": "NODE_ENV=production webpack",
    "build": "npm run build:server && npm run build:client",
    "postinstall": "npm run build",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development BABEL_ENV=node babel-watch src/index.js",
    "dev:api": "NODE_ENV=development BABEL_ENV=node SKIP_CLIENT=true babel-watch src/index.js",
    "dev:docker": "docker-compose up --build",
    "test": "NODE_ENV=test BABEL_ENV=node ava test",
    "test:dev": "NODE_ENV=test BABEL_ENV=node ava -w test",
    "lint": "eslint src test"
  },
  "keywords": [],
  "author": "Fela Maslen",
  "license": "MIT",
  "ava": {
    "require": [
      "@babel/register",
      "ignore-styles"
    ],
    "babel": {
      "testOptions": {
        "babelrc": true
      }
    }
  },
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "pg": "^7.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@epegzz/sass-vars-loader": "^4.3.1",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-watch": "^7.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-ava": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^14.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react-testing-library": "^6.1.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3"
  }
}
